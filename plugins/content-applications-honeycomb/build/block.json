{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/content-applications-honeycomb",
  "version": "0.1.0",
  "title": "Content Applications Honeycomb",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "classes": {
      "type": "string",
      "default": ""
    },
    "style": {
      "type": "string",
      "default": "padding:50px 0;overflow:hidden;"
    },
    "id": {
      "type": "string",
      "default": ""
    },
    "section_image": {
      "type": "number",
      "default": 0
    },
    "section_image_title": {
      "type": "string",
      "default": ""
    },
    "section_image_alt": {
      "type": "string",
      "default": ""
    },
    "section_image_class": {
      "type": "string",
      "default": "w-100 h-100 position-absolute bg-img"
    },
    "section_image_style": {
      "type": "string",
      "default": "top:0;left:0;object-fit:cover;pointer-events:none;"
    },
    "section_block": {
      "type": "string",
      "default": ""
    },
    "topImage": {
      "type": "number",
      "default": 0
    },
    "sidebarTitles": {
      "type": "array",
      "default": [
        {
          "title": "Title 1"
        },
        {
          "title": "Title 2"
        },
        {
          "title": "Title 3"
        }
      ]
    },
    "topRowGallery": {
      "type": "array",
      "default": [
        {
          "image": 0,
          "title": "Gallery Item 1"
        },
        {
          "image": 0,
          "title": "Gallery Item 2"
        },
        {
          "image": 0,
          "title": "Gallery Item 3"
        }
      ]
    },
    "bottomRowGallery": {
      "type": "array",
      "default": [
        {
          "image": 0,
          "title": "Gallery Item 1"
        },
        {
          "image": 0,
          "title": "Gallery Item 2"
        },
        {
          "image": 0,
          "title": "Gallery Item 3"
        }
      ]
    },
    "bottomImage": {
      "type": "number",
      "default": 0
    }
  },
  "textdomain": "content-applications-honeycomb",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}